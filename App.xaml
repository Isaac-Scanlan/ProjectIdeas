             xmlns:local="clr-namespace:ProjectIdeas"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:viewmodels="clr-namespace:ProjectIdeas.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/ProjectIdeas;component/Views/Themes/Generic.xaml"/>
